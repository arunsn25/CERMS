<config version="1.0">

<scope>
	<component id="cosdsearchframe">
		<pages>
			<start>/custom/jsp/library/component/search/cosdsearchframe.jsp</start>
		</pages>
		<class>com.cosd.greenbuild.calwin.web.library.search.CalwinSearchFrame</class>
		<nlsbundle>com.cosd.greenbuild.calwin.web.library.search.COSDSimpleSearchFrame</nlsbundle>
		<calwinsearchquery>
